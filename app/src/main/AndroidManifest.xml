<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.braingasm.in.braingasm">

    <application
        android:name=".custom.Calligraphy"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuestionPaperActivity" />
        <activity android:name=".EventsActivity" />
        <activity android:name=".NotesActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" android:parentActivityName=".LoginActivity" />
        <activity android:name=".UploadQuestionPaperActivity" android:parentActivityName=".QuestionPaperActivity" />
        <activity android:name=".CreateEventActivity" android:parentActivityName=".EventsActivity" />
        <activity android:name=".UploadNotesActivity" android:parentActivityName=".NotesActivity" />
    </application>

</manifest>